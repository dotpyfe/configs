! Transparency
URxvt.depth:         32
!This worked in XFCE, not in Xmonad?
!URxvt.background:    rgba:1111/1111/1111/dddd
URxvt.background:    rgba:0000/0000/0000/cccc

! scrollbar style - rxvt (default), plain (most compact), next, or xterm
!URxvt*scrollstyle:xterm
URxvt*scrollBar:     false
URxvt.saveLines:     32767

! Font
URxvt.font:          xft:Terminus:antialias=true:size=6
URxvt.boldFont:      xft:Terminus:style=bold:antialias=true:size=6

! Perl
URxvt.perl-ext-common:default,matcher

! Clickable Links
URxvt.urlLauncher:   google-chrome
URxvt.matcher.button:1 
URxvt.matcher.pattern.1: \\bwww\\.[\\w-]\\.[\\w./?&@#-]*[\\w/-]

! Icon
URxvt*iconFile:      /usr/share/icons/Tango/scalable/apps/xfce-terminal.svg

! Colors et. al.
URxvt*cursorColor:   darkorange1
URxvt*foreground:    grey75
URxvt*saveLines:     7777
URxvt*cursorBlink:   false
URxvt*termName:      rxvt-256color
URxvt*colorBD:       orange

! from davidbeckingsale

URxvt*colorUL:       #c5f779
URxvt*underlineColor:#c5f779
URxvt.urgentOnBell:  true

